var bd:BitmapData = new BitmapData(20, 10, true, 0x0);bd.fillRect(new Rectangle(0, 0, 10, 10), 0xFF0000FF);var vec:Vector.<uint> = bd.getVector(bd.rect);var i:int;var forwardsOrBackwards:Boolean = true;var reverse:Boolean = false;if (forwardsOrBackwards) {		for (i = 0; i < 10; i++) vec.splice(20 * i, 0, 0x0);		if (reverse) {		for (i = 0; i < 10; i++) vec.splice(20 * i, 1);	}} else {		for (i = 0; i < 11; i++) vec.unshift(0x0);	for (i = 0; i < 10; i++) vec.splice(20 * i + 1, 1);		if (reverse) {		for (i = 0; i < 10; i++) vec.splice(20 * i + 1, 0, 0x0);		for (i = 0; i < 11; i++) vec.shift();	}}bd.setVector(bd.rect, vec);addChild(new Bitmap(bd));width = 800;scaleY = scaleX;